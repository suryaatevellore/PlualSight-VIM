syntax on
 set number
 set ruler
 set background=dark
set nocompatible              " be iMproved, required
filetype off                  " required


nnoremap <C-J> <C-W><C-J>
nnoremap <C-K> <C-W><C-K>
nnoremap <C-L> <C-W><C-L>
nnoremap <C-H> <C-W><C-H>
au BufNewFile,BufRead *.py
    \ set tabstop=4 |
    \ set softtabstop=4 |
    \ set shiftwidth=4 |
    \ set textwidth=79 |
    \ set expandtab  |
    \ set autoindent | 
    \ set fileformat=unix
